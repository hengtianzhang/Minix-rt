#ifndef __KERNEL_VERSION_H_
#define __KERNEL_VERSION_H_

/* @templates@ values come from cmake/version.cmake */

#cmakedefine MINIX_RT_VERSION_CODE @MINIX_RT_VERSION_CODE@
#define MINIX_RT_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))

#define MINIXRTVERSION          @MINIXRTVERSION@
#define MINIX_RT_VERSION_NUMBER  @MINIX_RT_VERSION_NUMBER@
#define MINIX_RT_VERSION_MAJOR   @MINIX_RT_VERSION_MAJOR@
#define MINIX_RT_VERSION_MINOR   @MINIX_RT_VERSION_MINOR@
#define MINIX_RT_PATCHLEVEL      @MINIX_RT_PATCHLEVEL@
#define MINIX_RT_VERSION_STRING  @MINIX_RT_VERSION_STRING@

#endif /* !__KERNEL_VERSION_H_ */
